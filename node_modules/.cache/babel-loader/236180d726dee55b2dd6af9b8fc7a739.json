{"ast":null,"code":"import Custom_slider from \"@/components/Custom_slider\";\nexport default {\n  name: 'App',\n  components: {\n    Custom_slider\n  },\n  data: () => ({\n    currentTime: 5,\n    pages: []\n  }),\n  methods: {\n    parentMethodSlideNext() {\n      this.$refs.Custom_slider.slideNext();\n    },\n\n    parentMethodSlideBack() {\n      this.$refs.Custom_slider.slideBack();\n    },\n\n    startTimer() {\n      this.timer = setInterval(() => {\n        this.currentTime--;\n      }, 1000);\n    },\n\n    reloadTimer() {\n      clearTimeout(this.currentTime);\n      this.currentTime = 5;\n    }\n\n  },\n\n  mounted() {\n    this.startTimer();\n    this.pages = this.$refs.Custom_slider.slides;\n  },\n\n  watch: {\n    currentTime(time) {\n      if (time === 0) {\n        this.$refs.Custom_slider.slideNext();\n        this.reloadTimer();\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmBA;AAEA;EACAA,WADA;EAEAC;IACAC;EADA,CAFA;EAKAC;IACAC,cADA;IAEAC;EAFA,EALA;EASAC;IACAC;MACA;IACA,CAHA;;IAIAC;MACA;IACA,CANA;;IAQAC;MACA;QACA;MACA,CAFA,EAEA,IAFA;IAGA,CAZA;;IAaAC;MACAC;MACA;IACA;;EAhBA,CATA;;EA2BAC;IACA;IAEA;EACA,CA/BA;;EAgCAC;IACAT;MACA;QACA;QACA;MACA;IACA;;EANA;AAhCA","names":["name","components","Custom_slider","data","currentTime","pages","methods","parentMethodSlideNext","parentMethodSlideBack","startTimer","reloadTimer","clearTimeout","mounted","watch"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <div id=\"app\" class=\"content\">\n    <div class=\"form\">\n      <div class=\"content_head\">\n        <h1>Актуальное</h1>\n        <div class=\"sliderPage\">\n          <span v-bind:key=\"id\"  v-for=\"(item, id) in pages\" ></span>\n        </div>\n        <span>\n        <button @click=\"parentMethodSlideNext()\"> 1 </button>\n        <button @click=\"parentMethodSlideBack()\">2 </button>\n        </span>\n      </div>\n      <Custom_slider ref=\"Custom_slider\"/>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Custom_slider from \"@/components/Custom_slider\";\n\nexport default {\n  name: 'App',\n  components: {\n    Custom_slider,\n  },\n  data: () => ({\n    currentTime: 5,\n    pages: []\n  }),\n  methods: {\n    parentMethodSlideNext() {\n      this.$refs.Custom_slider.slideNext()\n    },\n    parentMethodSlideBack() {\n      this.$refs.Custom_slider.slideBack()\n    },\n\n    startTimer() {\n      this.timer = setInterval(() => {\n        this.currentTime--\n      }, 1000)\n    },\n    reloadTimer() {\n      clearTimeout(this.currentTime)\n      this.currentTime = 5\n    },\n  },\n  mounted() {\n    this.startTimer();\n\n    this.pages = this.$refs.Custom_slider.slides\n  },\n  watch: {\n    currentTime(time) {\n      if (time === 0) {\n        this.$refs.Custom_slider.slideNext()\n        this.reloadTimer()\n      }\n    }\n  },\n}\n</script>\n\n"]},"metadata":{},"sourceType":"module"}